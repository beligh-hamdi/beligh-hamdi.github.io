{
  "name": "beligh-hamdi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "copy": "node -e \"require('fs').copyFile('../docs/index.html', '../docs/404.html', (err) => console.log(err))\"",
    "postbuild": "npm run copy",
    "build": "ng build --output-path=../docs --base-href=https://beligh-hamdi.github.io/",
    "build:prod": "ng build --configuration production --base-href=https://beligh-hamdi.github.io/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^2.0.0",
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@angular/service-worker": "^16.0.0",
    "@angular/youtube-player": "^16.0.0",
    "@tinymce/tinymce-angular": "^7.0.0",
    "chart.js": "^4.2.1",
    "ngx-infinite-scroll": "^15.0.0",
    "ngx-online-status": "^2.0.0",
    "rxjs": "~7.8.0",
    "tinymce": "^6.4.2",
    "tslib": "^2.3.0",
    "video.js": "^8.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^15.0.4",
    "@types/jasmine": "~4.3.0",
    "@types/tinymce": "^4.6.5",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "typescript": "~4.9.4"
  }
}
